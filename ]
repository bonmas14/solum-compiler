list : struct<value_type> {
    s32 count;
    s32 size;
    value_type^ ptr;
}

value : list<s32> = create_list<s32>(10);

create_list : <value_type>(s32 size) -> <value_type> {
    ptr : value_type^ = allocate<value_type>(size);
}

list.add();
